
$('#my-articles').append('<%= j render partial: 'users/user_articles_btn', object: @user_articles %>');

$('.pagination').replaceWith('<%= j will_paginate @user_articles %>');

<% if @user_articles.next_page %>
    $('#with-button-article .pagination').hide(); // <--------
<% else %>
    $('#with-button-article .pagination, #load_more_articles').remove(); // <--------
<% end %>
